{
    "name": "wpint/wpint-plugin",
    "type": "project",
    "description": "The WPINT framework wordpress plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Ali Barzegar",
            "email": "ali.barzegar.rahimi@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "yoast/phpunit-polyfills": "^2.0"
    },
    "require": {
        "wpint/framework": "*",
        "wpint/inertia": "dev-dev"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../packages/wpint/route"
        },
        {
            "type": "path",
            "url": "../../packages/wpint/support"
        },
        {
            "type": "path",
            "url": "../../packages/wpint/wpapi"
        },
        {
            "type": "path",
            "url": "../../packages/wpint/framework"
        },
        {
            "type": "path",
            "url": "../../packages/wpint/inertia"
        }
    ],
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev"
}
